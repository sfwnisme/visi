{
  "name": "@sfwnisme/visi",
  "version": "1.0.0",
  "description": "solidjs conditional rendering components for reactjs",
  "keywords": [
    "react",
    "nextjs",
    "solidjs",
    "conditional rendering",
    "components",
    "visible",
    "shift",
    "case",
    "react-components",
    "react-library",
    "solidjs-inspired",
    "declarative",
    "react-utils",
    "react-conditional"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc"
  },
  "author": "sfwnisme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfwnisme/visi.git"
  },
  "homepage": "https://github.com/sfwnisme/visi#readme",
  "bugs": {
    "url": "https://github.com/sfwnisme/visi/issues"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <20.0.0",
    "react-dom": ">=16.8.0 <20.0.0"
  }
}
